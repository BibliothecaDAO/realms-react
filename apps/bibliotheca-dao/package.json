{
  "name": "Bibliotheca DAO App",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/BibliothecaForAdventurers/realms-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/BibliothecaForAdventurers/realms-react",
    "directory": "apps/atlas"
  },
  "author": {
    "name": "Bibliothecca",
    "url": "https://github.com/BibliothecaForAdventurers"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo ./.eslintcache && jest --clear-cache",
    "dev": "next",
    "build": "next build",
    "build-no-checks": "NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 next build",
    "vercel-build": "yarn share-static-hardlink && next build",
    "export": "next export",
    "start": "next start",
    "bundle-analyze": "cross-env ANALYZE=true yarn build",
    "?share-static-symlink": "echo 'Use this command to link assets, locales... from shared static folder'",
    "share-static-symlink": "rimraf ./public/shared-assets && symlink-dir ../../static/assets ./public/shared-assets",
    "?share-static-hardlink": "echo 'Use this command to link assets, locales... from shared static folder'",
    "share-static-hardlink": "rimraf ./public/shared-assets && syncdir ../../static/assets ./public/shared-assets --copy",
    "check-build-size": "size-limit --highlight-less",
    "check-dist": "es-check -v",
    "test": "run-s test-unit test-e2e",
    "test-unit": "jest --config jest.config.js",
    "test-e2e": "playwright test",
    "test-e2e-prod": "cross-env E2E_PLAYWRIGHT_MODE=BUILD_AND_START playwright test",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint-styles": "stylelint --config ./stylelint.config.js 'src/**/*.css'",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.9.2",
    "@next/bundle-analyzer": "12.2.2",
    "@next/env": "12.2.2",
    "@size-limit/file": "7.0.8",
    "@svgr/webpack": "6.3.0",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/line-clamp": "0.4.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "28.1.6",
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "autoprefixer": "10.4.7",
    "cross-env": "7.0.3",
    "es-check": "7.0.0",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-regexp": "1.7.0",
    "eslint-plugin-sonarjs": "0.14.0",
    "eslint-plugin-testing-library": "5.5.1",
    "jest": "28.1.3",
    "jest-css-modules-transform": "4.4.2",
    "next-transpile-modules": "9.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "size-limit": "7.0.8",
    "storybook-addon-next-router": "^4.0.0",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "26.0.0",
    "symlink-dir": "5.0.1",
    "sync-directory": "5.1.7",
    "tailwindcss": "3.1.6",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@babel/core": "7.18.9",
    "@bibliotheca-dao/core-lib": "workspace:^",
    "@bibliotheca-dao/postcss-100vh-fix": "^1.0.3",
    "@bibliotheca-dao/ui-lib": "workspace:^",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@headlessui/react": "1.6.6",
    "@heroicons/react": "^1.0.6",
    "@loaders.gl/core": "^3.2.5",
    "@playwright/test": "1.23.4",
    "@quentin-sommer/react-useragent": "3.2.0",
    "@tailwindcss/typography": "latest",
    "@types/three": "^0.142.0",
    "axios": "^0.27.2",
    "babel-plugin-glsl": "^1.0.0",
    "classnames": "^2.3.1",
    "gl-matrix": "^3.4.3",
    "graphql": "^16.5.0",
    "heroicons": "^1.0.6",
    "next": "12.2.2",
    "next-auth": "^4.10.2",
    "next-seo": "5.5.0",
    "picocolors": "1.0.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scroll": "1.8.7",
    "react-typist-component": "0.5.0",
    "sharp": "0.30.7"
  }
}
